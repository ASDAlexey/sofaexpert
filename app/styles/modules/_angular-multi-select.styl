.multiSelect
  .vertical
    float:none
  .horizontal:not(.multiSelectGroup)
    float:left
  .line
    max-height:34px
    overflow:hidden
  .acol
    osr()
    padding-left:12px
    width:100%
    display:inline-block
    min-width:12px
    white-space nowrap
    text-overflow ellipsis
    overflow hidden
    padding-top:5px
  .inlineBlock
    display:inline-block
  .button
    osr(14px)
    width:352px
    height:25px
    display:block
    position:relative
    text-align:center
    cursor:pointer
    border-bottom:1px solid #cab558
    padding:4px 20px 1px 8px
    font-size:14px
    color:#000
    -webkit-user-select:none
    -moz-user-select:none
    -ms-user-select:none
    -o-user-select:none
    user-select:none
    white-space:normal
    background-color:#fff
    &:after
      content:''
      position:absolute
      z-index:1
      top:50%
      right:10px
      sprite($select_arrow)
      tt(0,-50%)
    .buttonLabel
      padding-left:10px
      text-align:left
      width:100%
      overflow hidden
      text-overflow ellipsis
  //background-image:linear-gradient(white, #f7f7f7)
  .button:hover
    //      background-image:linear-gradient(#fff, #e9e9e9);
  .buttonClicked
    //      box-shadow:0 2px 5px rgba(0, 0, 0, 0.15) inset, 0 1px 2px rgba(0, 0, 0, 0.05);
  .buttonLabel
    display:inline-block
    padding:0
    white-space nowrap
    overflow hidden
    text-overflow ellipsis
  .caret
    display:inline-block
    width:0
    height:0
    margin:0 0 1px 12px !important
    vertical-align:middle
    border-top:4px solid #fff
    border-right:4px solid transparent
    border-left:4px solid transparent
    border-bottom:0 dotted
  .checkboxLayer
    background-color:#fff
    position:absolute
    z-index:999
    border:1px solid #cab558
    //-webkit-box-shadow:0 6px 12px rgba(0, 0, 0, 0.175)
    //box-shadow:0 6px 12px rgba(0, 0, 0, 0.175)
    //border-radius:0
    display:none !important
  .helperContainer
    border-bottom:1px solid #ddd
    padding:8px 8px 0 8px
  .helperButton
    display:inline
    text-align:center
    cursor:pointer
    border:1px solid #ccc
    height:26px
    font-size:13px
    border-radius:2px
    color:#666
    background-color:#f1f1f1
    line-height:1.6
    margin:0 0 8px 0
  .clearButton
    position:absolute
    display:inline
    text-align:center
    cursor:pointer
    border:1px solid #ccc
    height:22px
    width:22px
    font-size:13px
    border-radius:2px
    color:#666
    background-color:#f1f1f1
    line-height:1.4
    right:2px
    top:2px
  .inputFilter
    border-radius:2px
    border:1px solid #ccc
    height:26px
    font-size:14px
    width:100%
    padding-left:7px
    -webkit-box-sizing:border-box
    -moz-box-sizing:border-box
    box-sizing:border-box
    color:#888
    margin:0 0 8px 0
  .checkBoxContainer
    display:inline-block
    min-width:100%
  .show
    display:block !important
    min-width 100%
  .multiSelectItem
    width:217px
    display:block
    padding:3px
    color:#444
    white-space:nowrap
    user-select:none
    border:1px solid transparent
    position:relative
    tn(.5s)
  .multiSelectItem:not(.multiSelectGroup).selected
    background-color:#cab558
    color:#555
    cursor:pointer
    border-top:1px solid #cab558
    border-left:1px solid #cab558
    border-right:1px solid #cab558
  .multiSelectFocus
    background-image:linear-gradient(#c1c1c1, #999) !important
    color:#fff !important
    cursor:pointer
    border:1px solid #ccc !important
  .multiSelectGroup
    display:block
    clear:both
  .tickMark
    display:inline-block
    position:absolute
    right:10px
    font-size:10px
    top:50%
    margin-top:-4px
  .checkbox
    color:#ddd !important
    position:absolute
    left:-9999px
    cursor:pointer
  img
    vertical-align:middle
    margin-bottom:0
    height:22px
    width:22px
.multiSelect .clearButton:hover, .multiSelect .helperButton:hover
  //border:1px solid #ccc
  color:#999
  background-color:#f4f7e7
.multiSelect .clearButton:focus, .multiSelect .helperButton:focus, .multiSelect .inputFilter:focus
  //border:1px solid #66afe9 !important
  //box-shadow:inset 0 0px 1px rgba(0, 0, 0, 0.035), 0 0 5px rgba(82, 168, 236, 0.7) !important
.multiSelect .multiSelectItem:hover, .multiSelect .multiSelectGroup:hover
  //background-image:linear-gradient(#c1c1c1, #999) !important
  color:#fff !important
  cursor:pointer
  background-color:#f4f7e7
//border:1px solid #ccc !important
.multiSelect .multiSelectItem span:hover, .multiSelect .multiSelectGroup span:hover
  cursor:pointer
.multiSelect .disabled, .multiSelect .disabled:hover, .multiSelect .disabled label input:hover ~ span
  color:#c4c4c4 !important
  cursor:not-allowed !important
.checkboxLayer
  -webkit-animation-duration:.2s
  animation-duration:.2s
.inner-popup .acol label span
  width:100%
  white-space nowrap
  text-overflow ellipsis
  overflow hidden
  padding-top:3px
.calc-form > div:not(.wrapper-button):not(.wrapper-files-block).presence_project .multiSelect .button .buttonLabel
  padding-left:34px;
  text-overflow:ellipsis;
  white-space:nowrap;
  overflow:hidden;
  padding-right:24px;
.select-block
  margin-right:10px
  position:relative
  dibt()
  z-index:1
  &.open
    z-index:2
.multiSelect
  dibt()
  position:relative
  z-index:0
  .multiSelectItem
    width:100%
.tickMark
  display:none !important
.multiSelect .checkboxLayer
  top:24px